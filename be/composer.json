{
    "name": "n0099/tbm",
    "license": "AGPL-3.0-or-later",
    "type": "project",
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.7",
        "laravel/framework": "^10.13.1",
        "laravel/sanctum": "^3.2.5",
        "laravel/tinker": "^2.8.1",
        "google/protobuf": "^3.23.2",
        "google/recaptcha": "^1.3",
        "hedronium/spaceless-blade": "^3.0",
        "predis/predis": "^2.1.2",
        "spatie/regex": "^3.1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.22.0",
        "laravel/pint": "^1.10",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.5.2",
        "phpunit/phpunit": "^10.2",
        "spatie/laravel-ignition": "^2.1.3",
        "barryvdh/laravel-debugbar": "^3.8.1",
        "barryvdh/laravel-ide-helper": "^2.13",
        "jetbrains/phpstorm-attributes": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.17",
        "squizlabs/php_codesniffer": "^3.7.2",
        "phpmd/phpmd": "^2.13",
        "nunomaduro/larastan": "^2.6",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan-deprecation-rules": "^1.1.3",
        "phpstan/phpstan-strict-rules": "^1.5.1",
        "vimeo/psalm": "^5.12",
        "psalm/plugin-laravel": "^2.8",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "TbClient\\": "../tbclient.protobuf/tbm_php/TbClient/",
            "GPBMetadata\\": "../tbclient.protobuf/tbm_php/GPBMetadata/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
